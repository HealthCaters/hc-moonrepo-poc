{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "incremental": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "../../.moon/cache/types/apps/expo",
    "paths": {
      "@hc/expo/*": ["./*"],
      "@hc/shared/*": ["../../packages/shared/src/*"]
    }
  },
  "include": [".", "../../packages/shared/**/*"],
  "references": [
    {
      "path": "../../packages/shared"
    }
  ]
}
