{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "incremental": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "strict": true,
    "outDir": "../../.moon/cache/types/apps/expo",
    "paths": {
      "@hc/shared-models/*": ["../../shared/models/src/*"],
      "@hc/shared-utils/*": ["../../shared/utils/src/*"]
    }
  },
  "include": ["**/*", "../../shared/models/src/**/*", "../../shared/utils/src/**/*"],
  "references": [
    {
      "path": "../../shared/models"
    },
    {
      "path": "../../shared/utils"
    }
  ]
}
